{
  "name": "poc-react-ink",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "ink": "^2.0.0-12",
    "react": "^16.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.10",
    "rimraf": "^2.6.3",
    "standard": "^12.0.1"
  },
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "babel src -d lib",
    "watch": "babel --watch src -d lib",
    "serve": "nodemon lib --delay 500ms",
    "_start": "concurrently \"npm:watch\" \"npm:serve\"",
    "start": "node lib"
  },
  "author": "Matt Styles",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  }
}
